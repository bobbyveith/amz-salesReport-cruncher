{
    "manifest_version": 3,
    "name": "File Uploader",
    "version": "1.0",
    "permissions": [
        "storage",
        "activeTab",
        "downloads"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}